{
  "name": "@baloise/web-app-ng-utils",
  "version": "1.0.1",
  "description": "Utilities for Baloise Web Applications made with Angular",
  "scripts": {
    "lint": "eslint src --ext .ts,vue",
    "format": "npm run prettier:write",
    "prettier:write": "prettier --write \"./src\"",
    "prettier:check": "prettier --check \"./src\"",
    "semantic-release": "semantic-release",
    "build": "npm run clean && npm run build:compile && npm run clean:package",
    "build:compile": "ng-packagr -p package.json",
    "clean": "rimraf dist",
    "clean:package": "rimraf dist/package.json && rimraf dist/CHANGELOG.md && rimraf dist/LICENSE && rimraf dist/README.md"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/web-app-ng-utils.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/baloise/web-app-ng-utils/issues"
  },
  "homepage": "https://github.com/baloise/web-app-ng-utils#readme",
  "peerDependencies": {
    "@angular/common": ">=10.2.4",
    "@angular/core": ">=10.2.4",
    "@angular/forms": ">=10.2.4",
    "@angular/platform-browser": ">=10.2.4",
    "@angular/router": ">=10.2.4",
    "@baloise/design-system-components-angular": ">=1.6.2",
    "@baloise/web-app-google-utils": ">=1.2.0",
    "ag-grid-community": ">=25.3.0",
    "lodash": ">=4.17.21",
    "query-string": ">=7.0.1",
    "zone.js": ">=0.8.26"
  },
  "devDependencies": {
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@baloise/design-system-components-angular": "^1.6.2",
    "@baloise/web-app-google-utils": "^1.2.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "ag-grid-community": "^25.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "ng-packagr": "^10.1.2",
    "prettier": "^2.3.0",
    "query-string": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.0.8",
    "zone.js": "^0.10.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "main": "dist/bundles/baloise-web-app-ng-utils.umd.js",
  "module": "dist/fesm2015/baloise-web-app-ng-utils.js",
  "es2015": "dist/fesm2015/baloise-web-app-ng-utils.js",
  "esm2015": "dist/esm2015/baloise-web-app-ng-utils.js",
  "fesm2015": "dist/fesm2015/baloise-web-app-ng-utils.js",
  "typings": "dist/baloise-web-app-ng-utils.d.ts",
  "metadata": "dist/baloise-web-app-ng-utils.metadata.json",
  "sideEffects": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "umdModuleIds": {
        "@baloise/design-system-components": "@baloise/design-system-components",
        "@baloise/design-system-components/loader": "@baloise/design-system-components/loader",
        "@baloise/design-system-components-angular": "@baloise/design-system-components-angular",
        "lodash": "lodash",
        "query-string": "queryString"
      }
    },
    "whitelistedNonPeerDependencies": [
      "@baloise/design-system-components"
    ]
  }
}
